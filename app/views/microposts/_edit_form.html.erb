<%= form_for @information, :url => { :action => 'update' },remote: true do |f| %>

  <%= render 'shared/error_messages', object: f.object %>

    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :description %>
    <%= f.text_area :description, :rows => 4, :cols => 30  %>

  <br/>
  <br/>

    <%= f.submit %>

<% end %>
